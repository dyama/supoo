
; ookiihou wo kaesu max kansuu
defun ooki (x y)
  if (> x y)
    x
    y

; fibonacchi suu wo kaesu
defun fib (n)
  if (< n 2)
    n
    + (fib (- n 1)) (fib (- n 2))

dotimes (i 5)
  princ (fib i)
  format t " "

setq hoge
  cons 1 'a

print hoge

